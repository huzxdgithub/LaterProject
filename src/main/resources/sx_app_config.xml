	<!-- redis缓存集群和连接池配置项 -->
	<redis-additional-config
		type="com.sx.conf.RedisInitConfig">
		<!-- 是否集群,默认为false -->
		<cluster>true</cluster>
		<!-- 开启读写分离，default false，非集群主从模式下有效 -->
		<rwseparation>false</rwseparation>
		<!-- 非集群模式下redis密码 -->
		<password></password>
		<!-- 最大连接 -->
		<maxTotal>9</maxTotal>
		<!-- 最大空闲 -->
		<maxIdle>9</maxIdle>
		<!-- 最小空闲 -->
		<minIdle>7</minIdle>
	</redis-additional-config>
