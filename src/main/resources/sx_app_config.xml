	<!-- 多数据源配置信息 -->
	<multi-db-config type="com.sx.conf.MultiDbConfig">
		<item>
			<dbs>gyrlzywsqlserver</dbs>
			<cfgfile>gyrlzyw.xml</cfgfile>
			<driver>com.microsoft.jdbc.sqlserver.SQLServerDriver</driver>
